<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="feedPane" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.belsign.GUI.CameraViewController">
   <children>
      <ImageView fx:id="camFeed" fitHeight="150.0" pickOnBounds="true" preserveRatio="true" />
      <Button mnemonicParsing="false" onAction="#onCap" text="Take Picture" StackPane.alignment="CENTER_RIGHT">
         <padding>
            <Insets right="20.0" />
         </padding>
         <StackPane.margin>
            <Insets right="10.0" />
         </StackPane.margin>
      </Button>
   </children>
</StackPane>
